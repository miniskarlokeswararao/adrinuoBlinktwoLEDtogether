//
//#Blink two LEDâ€™s together with a delay of 5secs through Arduino?
int led1=13;
int led2=12;
void setup()
{
  pinMode(led1, OUTPUT);
    pinMode(led2, OUTPUT);
}

void loop()
{
  digitalWrite(led1, HIGH);
  digitalWrite(led2, HIGH);
  delay(5000); // Wait for 5 second(s)
  digitalWrite(led1, LOW);
  digitalWrite(led2, LOW);
  
}
